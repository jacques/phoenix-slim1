{
    "name": "jacques/phoenix-slim1",
    "description": "Shims for migrating to Slim 3.x from Slim 1.x and 2.x",
    "license": "MPL-2.0",
    "authors": [
        {
            "name": "Jacques Marneweck",
            "email": "jacques@siberia.co.za"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "brick/math": "^0.9.3",
        "illuminate/database": "^8.81",
        "illuminate/events": "^8.81",
        "illuminate/pagination": "^8.81",
        "monolog/monolog": "^2.3",
        "ramsey/uuid": "^4.2",
        "slim/slim": "^1.6",
        "smarty/smarty": "^3.1.44 || ^4.0.4",
        "swiftmailer/swiftmailer": "^6.3",
        "vlucas/valitron": "^1.4",
        "wikimedia/ip-set": "^3.0"
    },
    "require-dev": {
        "brick/varexporter": "^0.3.5",
        "ergebnis/composer-normalize": "^2.23",
        "fzaninotto/faker": "^1.9",
        "phpunit/php-invoker": "^3.1",
        "phpunit/phpunit": "^9.5",
        "rector/rector": "^0.12.15",
        "squizlabs/php_codesniffer": "^3.6",
        "vimeo/psalm": "^4.19"
    },
    "autoload": {
        "psr-4": {
            "Jacques\\Phoenix\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        },
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors --debug --configuration=tests/phpunit.xml --coverage-clover build/logs/clover.xml --coverage-text"
    }
}
