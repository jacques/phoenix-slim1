{
    "name": "jacques/phoenix-slim1",
    "description": "Shims for migrating to Slim 3.x from Slim 1.x and 2.x",
    "license": "MPL-2.0",
    "authors": [
        {
            "name": "Jacques Marneweck",
            "email": "jacques@siberia.co.za"
        }
    ],
    "require": {
        "php": "^8.0",
        "brick/math": "^0.10.2",
        "illuminate/database": "^10.48",
        "illuminate/events": "^10.48",
        "illuminate/pagination": "^10.48",
        "monolog/monolog": "^3.6",
        "ramsey/uuid": "^4.7",
        "slim/slim": "^1.6",
        "smarty/smarty": "^4.5.3",
        "symfony/mailer": "^6.4",
        "vlucas/valitron": "^1.4",
        "wikimedia/ip-set": "^3.1"
    },
    "require-dev": {
        "brick/varexporter": "^0.4.0",
        "ergebnis/composer-normalize": "^2.42",
        "phpunit/php-invoker": "^4.0",
        "phpunit/phpunit": "^10.5",
        "rector/rector": "^1.0",
        "squizlabs/php_codesniffer": "^3.10",
        "vimeo/psalm": "^5.24"
    },
    "autoload": {
        "psr-4": {
            "Jacques\\Phoenix\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        },
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors --debug --configuration=tests/phpunit.xml --coverage-clover build/logs/clover.xml --coverage-text"
    }
}
