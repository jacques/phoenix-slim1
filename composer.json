{
    "name": "jacques/phoenix-slim1",
    "description": "Shims for migrating to Slim 3.x from Slim 1.x and 2.x",
    "license": "MPL-2.0",
    "authors": [
        {
            "name": "Jacques Marneweck",
            "email": "jacques@siberia.co.za"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "brick/math": "^0.9.3",
        "illuminate/database": "^5.8 || ^9.0",
        "illuminate/events": "^5.8 || ^9.0",
        "illuminate/pagination": "^5.8 || ^9.0",
        "monolog/monolog": "^1.24 || ^2.7",
        "ramsey/uuid": "^3.9 || ^4.3",
        "slim/slim": "^1.6",
        "smarty/smarty": "^3.1.44 || ^4.0.4",
        "swiftmailer/swiftmailer": "^6.3",
        "vlucas/valitron": "^1.4",
        "wikimedia/ip-set": "^3.1"
    },
    "require-dev": {
        "brick/varexporter": "^0.3.6",
        "ergebnis/composer-normalize": "^2.28",
        "phpunit/php-invoker": "^3.1",
        "phpunit/phpunit": "^9.5",
        "rector/rector": "^0.13.6",
        "squizlabs/php_codesniffer": "^3.7",
        "vimeo/psalm": "^4.23"
    },
    "autoload": {
        "psr-4": {
            "Jacques\\Phoenix\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        },
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors --debug --configuration=tests/phpunit.xml --coverage-clover build/logs/clover.xml --coverage-text"
    }
}
