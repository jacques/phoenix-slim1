{
    "name": "jacques/phoenix-slim1",
    "description": "Shims for migrating to Slim 3.x from Slim 1.x and 2.x",
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "require": {
        "php": ">=7.0.0",
        "illuminate/database": "^5.7",
        "illuminate/events": "^5.7",
        "illuminate/pagination": "^5.7",
        "monolog/monolog": "^1.23",
        "moontoast/math": "^1.1",
        "ramsey/uuid": "^3.8",
        "slim/slim": "^1.6",
        "smarty/smarty": "^3.1",
        "swiftmailer/swiftmailer": "^6.1",
        "vlucas/valitron": "^1.4",
        "wikimedia/ip-set": "^2.0"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.8",
        "phpro/grumphp": "^0.15.0",
        "phpunit/php-invoker": "^2.0",
        "phpunit/phpunit": "^8.0",
        "rector/rector": "^0.5.5",
        "squizlabs/php_codesniffer": "^3.4",
        "vimeo/psalm": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "Jacques\\Phoenix\\": "src/"
        }
    },
    "license": "MPL-2.0",
    "authors": [
        {
            "name": "Jacques Marneweck",
            "email": "jacques@siberia.co.za"
        }
    ],
    "scripts": {
        "test": "vendor/bin/phpunit --colors --debug --configuration=tests/phpunit.xml --coverage-clover build/logs/clover.xml --coverage-text"
    }
}
